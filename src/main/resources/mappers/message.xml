<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="cg.park.springlotto.daos.MessageDao">
    <select id="selectMessage" parameterType="String" resultType="cg.park.springlotto.models.MessageDto">
        SELECT code, message
        FROM messageMst
        WHERE code = #{code}
    </select>
</mapper>